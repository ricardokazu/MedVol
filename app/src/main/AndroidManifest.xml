<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kazu.medvol">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Menus.ActivityMainMenu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Start.ActivityLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Start.ActivityRegister" />
        <activity android:name=".ActivityTrash" />
        <activity android:name=".List.ActivityNewItem">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Menus.ActivityMainMenu" />
        </activity>
        <activity android:name=".List.ActivityList" />
        <activity android:name=".List.ActivityItemInfo" />
        <activity android:name=".Menus.ActivityEditMenu" />
        <activity android:name=".List.ActivityRemoveItem" />
        <activity android:name=".List.ActivityGraph" />
        <activity android:name=".Extra.ActivityCredits" />
        <activity android:name=".Info.ActivityGeneralInfo" />
        <activity android:name=".Info.ActivityHelp" />
        <activity android:name=".List.ActivityCalibrate"></activity>

    </application>

</manifest>